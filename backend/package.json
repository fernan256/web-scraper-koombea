{
  "name": "web-scraper-backend",
  "version": "1.0.0",
  "description": "NodeJS scraper",
  "main": "backend/src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
